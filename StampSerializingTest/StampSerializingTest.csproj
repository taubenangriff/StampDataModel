    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\StampDataModel\StampDataModel.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="FileDBSerializer">
      <HintPath>lib\FileDBSerializer.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Update="stamp.filedb">
